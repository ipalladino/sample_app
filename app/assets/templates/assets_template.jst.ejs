<input type="file" name="file" value="" id="file" style="display:none;" />
<div class="row-fluid">
      <div id="uploaded_files">
          
<% _.each(items, function(item) { %>
    <div class="uploaded_img_container">
          <i class="icon-plus"></i>
          <% if(item.list != undefined) { %>
              <i class="icon-trash_can delete-asset-icon" data-id=<%= item.id %> ></i>
              <img src="<%= item.list %>" />
          <% } else { %>
              <img  />
          <% }  %>
    </div>
<% }) %>


    </div>
</div>
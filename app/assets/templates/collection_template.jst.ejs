<div class="span12">
    <span class="results"> Results <%= fr %> to <%= to %></span>
</div>

<div class="span12 paginations">
<% var aclass = "" %>
<% for(i=0;i<=total;i++) { %>
    <% if(i == page) { %>
        <span class="page-button dopage page-selected" data-page=<%= i %> ><%= i %></span>
    <% } else { %>
        <span class="page-button dopage" data-page=<%= i %> ><%= i %></span>
    <% } %>
<% } %>
<span class="page-button doleft"><i class="icon-arrow_left"></i></span>
<span class="page-button doright"><i class="icon-arrow_right"></i></span>
</div>

<% _.each(items, function(item) { %>
<div class="span4" style="height:300px;">
    <div>
        <% if(item.assets_urls.length > 0) { %>
            <img src="http://www.examiner.com/images/blog/replicate/EXID9044/images/resized_steve_mcqueen_ferrari_stolen_jalopnik.jpg" style="height:226px">
        <% } else { %>
            <img src="http://www.examiner.com/images/blog/replicate/EXID9044/images/resized_steve_mcqueen_ferrari_stolen_jalopnik.jpg" style="height:226px">
        <% } %>
    </div>
    <div>
        <a href="http://<%= window.location.host %>/car_model/<%= item.id %>"><%= item.car_model %></a>
        <a href="http://<%= window.location.host %>/car_model/<%= item.id %>"><%= item.car_year_str %></a>
    </div>        
</div>
<% }) %>

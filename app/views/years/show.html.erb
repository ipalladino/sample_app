<% provide(:title, @year.car_year) %>
<div class="row">
  <div class="span12">
      <h2><%= @year.car_year %>
      <% if current_user.admin? %>
          | <%= link_to "delete", @year, method: :delete,
                                        data: { confirm: "You sure?" },
                                        id: @year.id %>
      <% end %>
      </h2>
      <% if @year.car_models.any? %>
          <h3>Ferrari Models (<%= @year.car_models.count %>)</h3>
          <ol class="microposts">
            <%= render @carmodels %>
          </ol>
          <%= will_paginate @carmodels %>
      <% end %>
  </div>
</div>